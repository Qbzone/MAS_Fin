<Window x:Class="Mas_Projekt_Koniec.StronaGlowna"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mas_Projekt_Koniec"
        mc:Ignorable="d"
        Title="StronaGlowna" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75" />
            <RowDefinition Height="75" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Label Margin="20,0,0,10" HorizontalAlignment="Center" FontSize="40" Grid.Row="0">Strona Główna</Label>
        <Button x:Name="ZapiszButton" Click="ZapiszButton_Click" Margin="20,0,0,10" Padding ="20" HorizontalAlignment="Center" FontSize="20" Grid.Row="2">Zapisz Pacjentów</Button>
    </Grid>
</Window>
